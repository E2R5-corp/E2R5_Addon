{
	"manifest_version": 3,
	"name": "E2R5",
	"description": "The best extension for 42 ",
	"version": "2.2.0",
	"content_scripts": [
		{
			"matches": [
				"https://profile.intra.42.fr/*"
			],
			"js": [
				"src/intra.js"
			]
		},
		{
			"matches": [
				"https://stud42.fr/*"
			],
			"js": [
				"src/stud42.js"
			]
		}
	],
	"action": {
		"default_popup": "src/popup.html",
		"default_icon": "assets/e2r5_test.png",
		"default_title": "E2R5"
	},
	"icons": {
		"128": "assets/e2r5_128.png"
	}
}