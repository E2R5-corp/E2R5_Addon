{
	"manifest_version": 2,

	"name": "E2R5_TV",
	"description": "E2R5 Extension",
	"version": "1.0",
	"background": {
		"scripts": ["link_button.js"]
	},
	"content_scripts": [
		{
		  "matches": ["https://intra.42.fr/*"],
		  "js": ["intra.js"]
		}
	  ],
	"permissions": ["tabs", "activeTab"],
	"browser_action": {
		"default_icon": "icon_128px.png",
		"default_title": "Go to E2R5_TV!"
	}
}
