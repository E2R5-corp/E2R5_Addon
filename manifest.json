{
	"manifest_version": 2,

	"name": "E2R5",
	"description": "Open E2R5_TV youtube page, The logo of the E2R5tv appears on the intra",
	"version": "1.3.0",
	"background": {
		"scripts": ["link_button.js"]
	},
	"content_scripts": [
		{
		  "matches": ["https://profile.intra.42.fr/*"],
		  "js": ["intra.js"]
		},
		{
		  "matches": ["https://stud42.fr/*"],
		  "js": ["stud42.js"]
		}
	  ],
	"permissions": ["tabs", "activeTab"],
	"browser_action": {
		"default_icon": "icon_128px.png",
		"default_title": "E2R5"
	}
}
